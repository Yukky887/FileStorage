<Window x:Class="FileClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="File Client" Height="450" Width="450">
    <StackPanel Margin="10">
        <Button Content="Показать файлы" Click="OnGetFileClick" Margin="0,0,0,10"/>
        <Button Content="Загрузить файл" Click="OnUploadFileClick" Margin="0,0,0,10"/>
        <ListBox x:Name="FilesList"/>
    </StackPanel>
</Window>
